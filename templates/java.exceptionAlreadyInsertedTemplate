package {{systemName|lower}}.ev.exception;

public class {{entity.name}}AlreadyInsertedException extends Exception {

	private int idEntity;

	public {{entity.name}}AlreadyInsertedException(int idEntity) {
		super(ExceptionMessages.EXC_ALREADY_EXISTS);
		this.idEntity = idEntity;
	}

	public int getid{{entity.name}}() {
		return idEntity;
	}
}
